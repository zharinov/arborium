repo "https://github.com/tree-sitter-grammars/tree-sitter-objc"
commit "181a81b8f23a2d593e7ab4259981f50122909fda"
license "MIT"

grammar {
    id "objc"
    name "Objective-C"
    tag "code"
    tier 3
    icon "devicon-plain:objectivec"
    aliases "objective-c" "mm"

    dependency "tree-sitter-c" crate="arborium-c"

    queries {
        highlights {
            prepend crate="arborium-c"
        }
    }

    inventor "Brad Cox and Tom Love"
    year 1984
    description "Object-oriented superset of C used for macOS/iOS; Apple's modern reference is the <a href=\"https://developer.apple.com/library/archive/documentation/Cocoa/Conceptual/ProgrammingWithObjectiveC/\">Programming with Objective-C guide</a>."
    link "https://en.wikipedia.org/wiki/Objective-C"
    trivia "Derived from Smalltalk messaging added to C; NeXT licensed it in 1988, leading to its adoption by Apple after 1996â€”documented in Apple's developer library."

    sample {
        path "samples/RCTLog.mm"
        description "React Native logging utility showing categories, macros, Objective-C classes, and C functions."
        link "https://github.com/facebook/react-native/blob/main/packages/react-native/React/Base/RCTLog.mm"
        license "MIT"
    }
}
